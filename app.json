{
  "expo": {
    "name": "artesanias",
    "slug": "Gamificacion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/identify.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/OAXACA.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/identify.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "CAMERA_ROLL",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.maskatezta.Gamificacion",
      "config":{
        "googleMaps":{
          "apiKey": "AIzaSyA_IdzKez5qoH52KFCHOk8UlLrsZB9BFOo"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Explore Place to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "92cbc4e1-a28a-4ded-8dad-11cc98055379"
      }
    }
  }
}
